  "build": {
    "beforeBuildCommand": "",
    "frontendDist": "../../frontend/dist",
    "beforeDevCommand": "",
    "devUrl": "http://localhost:5173"
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
